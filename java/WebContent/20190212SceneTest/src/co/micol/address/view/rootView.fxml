<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="350.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <children>
      <ImageView fitHeight="300.0" fitWidth="480.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets left="8.0" top="10.0" />
         </VBox.margin>
         <image>
            <Image url="@../../../../../images/javafx.jpg" />
         </image>
      </ImageView>
      <ButtonBar prefHeight="59.0" prefWidth="500.0">
         <buttons>
            <Button mnemonicParsing="false" text="이전" />
          <Button mnemonicParsing="false" text="다음" />
         </buttons>
      </ButtonBar>
   </children>
</VBox>
